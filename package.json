{
  "name": "jsbasics",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/uuid": "^8.3.4",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "build:mysqldocker": "docker build -t test-zode-mysql -f Dockerfile . && docker run -d -p 2002:3306 --name=test-zode-mysql test-zode-mysql",
    "start": "node .",
    "test": "jest",
    "dev": "nodemon app.ts",
    "pretest:integration": "docker start test-zode-mysql",
    "test:integration": "jest -c jest.config.integration.js || :",
    "posttest:integration": "docker stop test-zode-mysql",
    "test:integration:ci": "jest -c jest.config.integration.js",
    "migration:init": "sh -c 'knex init -x ts' ",
    "migration:createTable": "sh -c 'knex migrate:make ${0} -x ts' ",
    "migration:migrate": "sh -c 'knex migrate:latest' ",
    "migration:rollback": "sh -c 'knex migrate:rollback' ",
    "migration:rollback:all": "sh -c 'knex migrate:rollback --all' "
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {
      "\\.ts$": "ts-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akshayZopsmart/jsbasics.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akshayZopsmart/jsbasics/issues"
  },
  "homepage": "https://github.com/akshayZopsmart/jsbasics#readme",
  "dependencies": {
    "@types/node": "^17.0.35",
    "dotenv": "^16.0.1",
    "jest": "^28.1.0",
    "joi": "^17.6.0",
    "knex": "^2.0.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.16",
    "pg": "^8.7.3",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "uuid": "^8.3.2"
  }
}
